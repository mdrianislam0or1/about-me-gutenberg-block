{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "bradsboilerplate/interactivity-block",
  "version": "0.1.0",
  "title": "Interactivity Block",
  "category": "widgets",
  "icon": "smiley",
  "attributes": {
    "skyColor": {
      "type": "string"
    },
    "grassColor": {
      "type": "string"
    },
    "name": {
      "type": "string",
      "default": ""
    },
    "image": {
      "type": "string",
      "default": ""
    },
    "description": {
      "type": "string",
      "default": "About Me"
    },
    "tags": {
      "type": "array",
      "default": []
    },
    "counters": {
      "type": "array",
      "default": []
    },
    "clientComment": {
      "type": "array",
      "default": []
    }
  },
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "align": true,
    "html": false,
    "dimensions": {
      "aspectRatio": true,
      "minHeight": true
    },
    "alignWide": true,
    "color": {
      "background": true
    }
  },
  "textdomain": "interactivity-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScriptModule": "file:./view.js"
}